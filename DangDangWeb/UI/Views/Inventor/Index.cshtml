@model IList<Model.BiaoDanInfo>
 <div class="layui-form">
        <table class="layui-table">
            
            <thead>
                <tr>
                    <th>编号</th>
                    <th>类别</th>
                    <th>书名</th>
                    <th>数量</th>
                    <th>总价格</th>
                    <th>出版社</th>
                    <th>批次</th>
                    <th>时间</th>
                    <th>状态</th>
                </tr>
            </thead>
            <tbody>
                @foreach (Model.BiaoDanInfo list in Model)
                {
                  <tr>
                    <td>@list.Id</td>
                    <td>@list.BookType</td>
                    <td>@list.BookName</td>
                    <td>@list.Count</td>
                    <td>@list.Price</td>
                    <td>@list.Publisher</td>
                    <td>@list.PiCi</td>
                    <td>@list.DateTime</td>
                      <td>
                           <span class="layui-badge">已审核</span>
                      </td>
                </tr>
                }
              
               
            </tbody>
        </table>
    </div>
   